  "dependencies": {
    "boom": "^4.2.0",
    "hapi": "^15.2.0",
    "inert": "^4.0.2",
    "joi": "^10.0.1",
    "mongojs": "^2.4.0",
    "node-sass": "^3.13.0",
    "node-uuid": "^1.4.7",
    "nodemon": "^1.11.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
